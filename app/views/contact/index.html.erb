<div class="main-banner">
  <div class="main-banner-content text-center">
    <h1>Your Contacts</h1>
  </div>
</div>
<div class="wrapper">
  <div class="container text-center">
    <div class="row">
      <% @all_contacts.each do |contact| %>
        <div class="col-sm-6 col-xs-12">
          <%= render 'card', contact: contact %>
        </div>
      <% end %>
    </div>
  </div>
</div>

